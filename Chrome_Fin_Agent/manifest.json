{
  "manifest_version": 3,
  "name": "Stock Analysis Extension",
  "version": "1.0",
  "description": "Fetch financial analysis for a stock from FastAPI backend",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["activeTab"]
}
